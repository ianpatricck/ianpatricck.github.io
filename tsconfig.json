{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "module": "commonjs", 
    "rootDir": "./",
    "moduleResolution": "node10",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
